{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
 
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "jest":true
  },
  "rules": {
    "semi": 2,
    "react/prop-types": "off"
  }
}